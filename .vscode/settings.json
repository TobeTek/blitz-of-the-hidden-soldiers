{
	"search.exclude": {
		"export/**/*.hx": true
	},
	"[haxe]": {
		"editor.formatOnSave": true,
		"editor.formatOnPaste": true,
		"editor.codeActionsOnSave": {
			"source.sortImports": true
		}
	},
	"haxe.enableExtendedIndentation": true,
	"files.associations": {
		"cstddef": "cpp",
		"cstdio": "cpp",
		"cwchar": "cpp",
		"array": "cpp",
		"atomic": "cpp",
		"*.tcc": "cpp",
		"vector": "cpp",
		"functional": "cpp",
		"memory": "cpp",
		"memory_resource": "cpp",
		"optional": "cpp",
		"string_view": "cpp",
		"tuple": "cpp",
		"type_traits": "cpp",
		"random": "cpp",
		"fstream": "cpp",
		"initializer_list": "cpp",
		"new": "cpp",
		"stdexcept": "cpp",
		"string": "cpp",
		"system_error": "cpp",
		"typeinfo": "cpp",
		"utility": "cpp",
		"iostream": "cpp",
		"ostream": "cpp"
	}
}